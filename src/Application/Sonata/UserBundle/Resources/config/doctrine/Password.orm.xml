<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Sonata\UserBundle\Entity\Password">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="titulo" type="string" column="titulo" length="255"/>
        <field name="nombreUsuario" type="string" column="nombreUsuario" length="255"/>
        <field name="url" type="string" column="url" length="255"/>
        <field name="password" type="string" column="password" length="255"/>
        <field name="comentario" type="string" column="comentario" length="255"/>
        <field name="fechaExpira" type="datetime" column="fechaExpira"/>
        <field name="fechaCreacion" type="datetime" column="fechaCreacion"/>
        <field name="fechaModificacion" type="datetime" column="fechaModificacion"/>
        <field name="fechaUltimoAcceso" type="datetime" column="fechaUltimoAcceso"/>
        <many-to-many field="categorias" inversed-by="passwords" target-entity="CategoriaPass">
            <join-table name="passwords_categorias">
                <join-columns>
                    <join-column name="password_id" referenced-column-name="id" />
                </join-columns>
                <inverse-join-columns>
                    <join-column name="categoriapass_id" referenced-column-name="id" />
                </inverse-join-columns>
            </join-table>
        </many-to-many>
        <field name="tipoPassword" type="string" column="tipoPassword" length="255"/>
    </entity>
</doctrine-mapping>



  